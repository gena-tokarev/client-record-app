    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.13",
    "typeorm-fixtures-cli": "^3.1.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0"

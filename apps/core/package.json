    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/express-session": "^1.17.7",
    "@types/lodash": "^4.14.194",
    "Faker": "^0.7.2",
    "bcrypt": "^5.1.0",
